{
    "description": "Patch a inventory item template object.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "patch.dcim.inventory_item_templates",
    "parameters": {
        "component_id": {
            "description": "Component id",
            "required": false,
            "type": "integer"
        },
        "component_type": {
            "description": "Component type",
            "required": false,
            "type": "string"
        },
        "description": {
            "description": "Description",
            "required": false,
            "type": "string"
        },
        "device_type": {
            "description": "Device type",
            "required": false,
            "type": "object"
        },
        "endpoint_uri": {
            "default": "/dcim/inventory-item-templates/{{ id }}/",
            "immutable": true,
            "type": "string"
        },
        "fail_non_2xx": {
            "default": false,
            "description": "Set action as failed when http return reponse status isn't 2xx.",
            "type": "boolean"
        },
        "get_detail_route_eligible": {
            "default": true,
            "immutable": true,
            "type": "boolean"
