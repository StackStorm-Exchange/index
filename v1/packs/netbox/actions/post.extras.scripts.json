{
    "description": "Post a list of script objects.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "post.extras.scripts",
    "parameters": {
        "endpoint_uri": {
            "default": "/extras/scripts/",
            "immutable": true,
            "type": "string"
        },
        "fail_non_2xx": {
            "default": false,
            "description": "Set action as failed when http return reponse status isn't 2xx.",
            "type": "boolean"
        },
        "get_detail_route_eligible": {
            "default": true,
            "immutable": true,
            "type": "boolean"
        },
        "http_verb": {
            "default": "post",
            "immutable": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
