{
    "description": "Patch a bookmark object.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "patch.extras.bookmarks",
    "parameters": {
        "endpoint_uri": {
            "default": "/extras/bookmarks/{{ id }}/",
            "immutable": true,
            "type": "string"
        },
        "fail_non_2xx": {
            "default": false,
            "description": "Set action as failed when http return reponse status isn't 2xx.",
            "type": "boolean"
        },
        "get_detail_route_eligible": {
            "default": true,
            "immutable": true,
            "type": "boolean"
        },
        "http_verb": {
            "default": "patch",
            "immutable": true,
            "type": "string"
        },
        "id": {
            "description": "ID of the object to patch.",
            "required": true,
            "type": "integer"
        },
        "object_id": {
            "description": "Object id",
            "required": false,
            "type": "integer"
        },
        "object_type": {
            "description": "Object type",
            "required": false,
            "type": "string"
        },
        "user": {
            "description": "User",
            "required": false,
            "type": "object"
        }
    },
    "runner_type": "python-script"
}
