{
    "description": "returns an array of the row keys for a given sheet in the excel file",
    "enabled": true,
    "entry_point": "get_keys_for_rows.py",
    "name": "get_keys_for_rows",
    "parameters": {
        "excel_file": {
            "description": "Excel file name",
            "required": false,
            "type": "string"
        },
        "key_column": {
            "default": 1,
            "description": "Starting column for keys",
            "required": false,
            "type": "integer"
        },
        "sheet": {
            "description": "Excel sheet name",
            "required": true,
            "type": "string"
        },
        "strict": {
            "default": true,
            "description": "When enabled, Sheet must exist & Variable Names must exist",
            "required": false,
            "type": "boolean"
        },
        "variable_name_row": {
            "default": 1,
            "description": "Starting row for variable names",
            "required": false,
            "type": "integer"
        }
    },
    "runner_type": "python-script"
}
