    "pack": "consul",
    "parameters": {
        "acl_id": {
            "description": "Fetch information for 'acl_id'.",
            "required": true,
            "type": "string"
        },
        "consul_profile": {
            "description": "Consul profile to use to run the action.",
            "required": false,
            "type": "string"
        },
        "token": {
            "default": "",
            "description": "An ACL token to use instead of the agent token.",
            "required": false,
            "secret": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
