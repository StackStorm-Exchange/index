{
    "description": "Download config(s) from Solarwinds NCM Orion module.",
    "enabled": true,
    "entry_point": "ncm_config_download.py",
    "name": "ncm_config_download",
    "pack": "orion",
    "parameters": {
        "configs": {
            "default": [
                "running",
