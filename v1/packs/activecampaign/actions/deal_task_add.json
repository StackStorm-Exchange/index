            "type": "string"
        },
        "duedate": {
            "description": "Due date of the new deal task. Example: '2014-03-17 09:30:00'",
            "required": true,
            "type": "string"
        },
        "note": {
            "description": "Text of the new deal task. Example: 'Discuss merger specifics'",
            "required": false,
            "type": "string"
        },
        "owner": {
            "description": "ID of the owner of the new deal task. Example: '4' (Get available owner IDs with \"user_list\" call)",
