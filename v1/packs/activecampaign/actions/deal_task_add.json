        "api_action": {
            "default": "deal_task_add",
            "description": "deal_task_add",
            "required": false,
            "type": "string"
        },
        "api_key": {
            "description": "Your API key",
            "required": false,
            "type": "string"
        },
        "api_output": {
            "default": "json",
            "description": "json",
            "required": false,
            "type": "string"
