{
    "description": "Import basic message template.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "message_template_import",
    "parameters": {
        "api_action": {
            "default": "message_template_import",
            "description": "message_template_import",
            "required": false,
            "type": "string"
        },
        "api_key": {
            "description": "Your API key",
            "required": false,
            "type": "string"
        },
        "api_output": {
            "default": "json",
            "description": "xml, json, or serialize (default is XML)",
            "required": false,
            "type": "string"
        },
        "names": {
            "description": "Array of internal template names. Example: 'My Template' (each array item corresponds to the same item in the 'urls' array)",
            "required": true,
            "type": "object"
        },
        "p": {
            "default": {},
            "description": "1",
            "required": false,
            "type": "object"
        },
        "template_scope2": {
            "description": "List visibility. Examples: 'all', 'specific'",
            "required": true,
            "type": "string"
        },
        "urls": {
            "description": "Array of URL's to XML files. Example: http://mysite.com/template.xml",
            "required": true,
            "type": "object"
        }
    },
    "runner_type": "python-script"
}
