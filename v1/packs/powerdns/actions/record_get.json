{
    "description": "Get record data",
    "enabled": true,
    "entry_point": "record_get.py",
    "name": "record_get",
    "parameters": {
        "record_name": {
            "description": "Record name to be fetched.",
            "required": true,
            "type": "string"
        },
        "response_timeout": {
            "default": 5,
            "description": "Time to wait for a response from PowerDNS.",
            "required": true,
            "type": "integer"
        },
        "server_id": {
            "default": "localhost",
            "description": "Server name to query.",
            "required": true,
            "type": "string"
        },
        "zone_name": {
            "description": "Zone name to fetch.",
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
