        },
        "rule": {
            "description": "The name of the maintenance rule",
            "required": true,
            "type": "string"
        },
        "vserver": {
            "description": "The name of the Virtual Server",
            "required": true,
            "type": "string"
        },
        "vtm": {
            "description": "The vTM instance to configure",
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
