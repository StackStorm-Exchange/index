{
    "description": "Retrieve a permalink URL for a specific extant message",
    "enabled": true,
    "entry_point": "run.py",
    "name": "chat.getPermalink",
    "parameters": {
        "channel": {
            "required": true,
            "type": "string"
        },
        "end_point": {
            "default": "chat.getPermalink",
            "immutable": true,
            "type": "string"
        },
        "http_method": {
            "default": "GET",
            "required": true,
            "type": "string"
        },
        "message_ts": {
            "required": true,
            "type": "string"
        },
        "token": {
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
