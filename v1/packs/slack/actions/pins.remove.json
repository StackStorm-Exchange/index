{
    "description": "Un-pins an item from a channel.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "pins.remove",
    "parameters": {
        "channel": {
            "description": "Channel where the item is pinned to.",
            "required": false,
            "type": "string"
        },
        "end_point": {
            "default": "pins.remove",
            "immutable": true,
            "type": "string"
        },
        "file": {
            "description": "File to un-pin.",
            "required": false,
            "type": "string"
        },
        "file_comment": {
            "description": "File comment to un-pin.",
            "required": false,
            "type": "string"
        },
        "http_method": {
            "default": "POST",
            "required": true,
            "type": "string"
        },
        "timestamp": {
            "description": "Timestamp of the message to un-pin.",
            "required": false,
            "type": "number"
        },
        "token": {
            "description": "Authentication token. Requires scope: `pins:write`",
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
