{
    "description": "Fetches history of messages and events from a multiparty direct message.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "mpim.history",
    "parameters": {
        "channel": {
            "description": "Multiparty direct message to fetch history for.",
            "required": false,
            "type": "string"
        },
        "count": {
            "default": 100,
            "description": "Number of messages to return, between 1 and 1000.",
            "required": false,
            "type": "integer"
        },
        "end_point": {
            "default": "mpim.history",
            "immutable": true,
            "type": "string"
        },
        "http_method": {
            "default": "GET",
            "required": true,
            "type": "string"
        },
        "inclusive": {
            "default": 0,
            "description": "Include messages with latest or oldest timestamp in results.",
            "required": false,
            "type": "boolean"
        },
        "latest": {
            "default": "now",
            "description": "End of time range of messages to include in results.",
            "required": false,
            "type": "number"
        },
        "oldest": {
            "default": 0,
            "description": "Start of time range of messages to include in results.",
            "required": false,
            "type": "number"
        },
        "token": {
            "description": "Authentication token. Requires scope: `mpim:history`",
            "required": false,
            "type": "string"
        },
        "unreads": {
            "default": 0,
            "description": "Include `unread_count_display` in the output?",
            "required": false,
            "type": "boolean"
        }
    },
    "runner_type": "python-script"
}
