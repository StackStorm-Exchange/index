{
    "description": "Close a direct message channel.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "im.close",
    "parameters": {
        "channel": {
            "description": "Direct message channel to close.",
            "required": true,
            "type": "string"
        },
        "end_point": {
            "default": "im.close",
            "immutable": true,
            "type": "string"
        },
        "http_method": {
            "default": "POST",
            "required": true,
            "type": "string"
        },
        "token": {
            "description": "Authentication token. Requires scope: `im:write`",
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
