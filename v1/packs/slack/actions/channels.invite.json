    "name": "channels.invite",
    "parameters": {
        "channel": {
            "required": true,
            "type": "string"
        },
        "end_point": {
            "default": "channels.invite",
            "immutable": true,
            "type": "string"
        },
        "http_method": {
            "default": "POST",
            "required": true,
            "type": "string"
        },
        "token": {
            "required": false,
            "type": "string"
        },
        "user": {
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
