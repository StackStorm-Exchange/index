{
    "description": "Invites users to a channel.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "conversations.invite",
    "parameters": {
        "channel": {
            "description": "The ID of the public or private channel to invite user(s) to.",
            "required": false,
            "type": "string"
        },
        "end_point": {
            "default": "conversations.invite",
            "immutable": true,
            "type": "string"
        },
        "http_method": {
            "default": "POST",
            "required": true,
            "type": "string"
        },
        "token": {
            "description": "Authentication token. Requires scope: `conversations:write`",
            "required": false,
            "type": "string"
        },
        "users": {
            "description": "A comma separated list of user IDs. Up to 1000 users may be listed.",
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
