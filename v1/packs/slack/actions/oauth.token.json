{
    "description": "Exchanges a temporary OAuth verifier code for a workspace token.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "oauth.token",
    "parameters": {
        "client_id": {
            "required": true,
            "type": "string"
        },
        "client_secret": {
            "required": true,
            "type": "string"
        },
        "code": {
            "required": true,
            "type": "string"
        },
        "end_point": {
            "default": "oauth.token",
            "immutable": true,
            "type": "string"
        },
        "redirect_uri": {
            "required": false,
            "type": "string"
        },
        "single_channel": {
            "default": "false",
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
