{
    "description": "Gets information about a bot user.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "bots.info",
    "parameters": {
        "bot": {
            "description": "Bot user to get info on",
            "required": false,
            "type": "string"
        },
        "end_point": {
            "default": "bots.info",
            "immutable": true,
            "type": "string"
        },
        "http_method": {
            "default": "GET",
            "required": true,
            "type": "string"
        },
        "token": {
            "description": "Authentication token. Requires scope: `users:read`",
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
