{
    "description": "Fetches a conversation's history of messages and events.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "conversations.history",
    "parameters": {
        "channel": {
            "required": true,
            "type": "string"
        },
        "cursor": {
            "required": false,
            "type": "string"
        },
        "end_point": {
            "default": "conversations.history",
            "immutable": true,
            "type": "string"
        },
        "inclusive": {
            "default": "0",
            "required": false,
            "type": "string"
        },
        "latest": {
            "default": "now",
            "required": false,
            "type": "string"
        },
        "limit": {
            "default": "100",
            "required": false,
            "type": "string"
        },
        "oldest": {
            "default": "0",
            "required": false,
            "type": "string"
        },
        "token": {
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
