{
    "description": "Provide custom unfurl behavior for user-posted URLs",
    "enabled": true,
    "entry_point": "run.py",
    "name": "chat.unfurl",
    "parameters": {
        "channel": {
            "required": true,
            "type": "string"
        },
        "end_point": {
            "default": "chat.unfurl",
            "immutable": true,
            "type": "string"
        },
        "token": {
            "required": false,
            "type": "string"
        },
        "ts": {
            "required": true,
            "type": "string"
        },
        "unfurls": {
            "required": true,
            "type": "string"
        },
        "user_auth_message": {
            "required": false,
            "type": "string"
        },
        "user_auth_required": {
            "default": "0",
            "required": false,
            "type": "string"
        },
        "user_auth_url": {
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
