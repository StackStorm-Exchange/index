{
    "description": "Pins an item to a channel.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "pins.add",
    "parameters": {
        "channel": {
            "description": "Channel to pin the item in.",
            "required": false,
            "type": "string"
        },
        "end_point": {
            "default": "pins.add",
            "immutable": true,
            "type": "string"
        },
        "http_method": {
            "default": "POST",
            "required": true,
            "type": "string"
        },
        "timestamp": {
            "description": "Timestamp of the message to pin.",
            "required": false,
            "type": "number"
        },
        "token": {
            "description": "Authentication token. Requires scope: `pins:write`",
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
