{
    "description": "Updates the admin status and description for a specified agent.",
    "enabled": true,
    "entry_point": "src/wrapper.py",
    "name": "agent-update",
    "parameters": {
        "admin_state_down": {
            "default": true,
            "description": "Set admin state up of the agent to false.",
            "type": "boolean"
        },
        "base": {
            "default": "agent-update",
            "immutable": true,
            "type": "string"
        },
        "cloud": {
            "description": "A specific cloud to query",
            "type": "string"
        },
        "description": {
            "description": "Description for the agent.",
            "type": "string"
        },
        "ep": {
            "default": "EntryPoint.parse('agent-update = neutronclient.neutron.v2_0.agent:UpdateAgent')",
            "immutable": true,
            "type": "string"
        },
        "id": {
            "description": "ID of agent to update.",
            "required": true,
            "type": "string"
        },
        "project_id": {
            "description": "Run the action under a different project, identified by id",
            "type": "string"
        },
        "project_name": {
            "description": "Run the action under a different project, identified by name",
            "type": "string"
        },
        "request_format": {
            "description": "==SUPPRESS== (choices: json)",
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
